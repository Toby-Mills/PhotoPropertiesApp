<?xml version="1.0" encoding="utf-8" ?> 
<configuration>
    <appSettings>
		<!-- InitializationXmlFile                                          -->
		<!--   If included, the XML file would be used to initialize the    -->
		<!--   PhotoPropertiesLibrary. If not included, the stored XML      -->
		<!--   resource is used.                                            -->
<!--
		<add key="InitializationXmlFile" value="PhotoMetadata.xml" />
-->

		<!-- OutputXmlNamespace                                             -->
		<!--   If included, it provides the namespace for the output        -->
		<!--   XML file.                                                    -->
        <add key="OutputXmlNamespace" value="http://tempuri.org/PhotoPropertyOutput.xsd" />

		<!-- OutputXmlNamespace                                             -->
		<!--   If included, it provides the XSL transform file instruction  -->
		<!--   for the output XML file.                                     -->
        <add key="OutputXSLTransform" value="PhotoPropertyOutput.xslt" />
    </appSettings>
</configuration>
